{
  "name": "lsst-daq",
  "version": "2.1.0",
  "author": "AURA/LSST",
  "summary": "Installs and configures LSST DAQ software",
  "license": "Apache-2.0",
  "source": "https://github.com/lsst-it/puppet-daq",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
